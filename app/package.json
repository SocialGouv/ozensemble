{
  "name": "oz_ensemble",
  "version": "1.25.1",
  "private": true,
  "scripts": {
    "get-ip": "./get-ip.sh && echo \"this script updates the API IP to your own IP, because that's what Android Emulator needs to work. It's based on your wifi IP, if you need ethernet it won't work (needs en1 instead of en0)\"",
    "android": "react-native run-android",
    "ios": "ENVFILE=.env.local react-native run-ios  --simulator=\"iPhone 15 Pro Max\"",
    "postinstall": "npx patch-package && react-native setup-ios-permissions && pod install --project-directory=ios/ --repo-update  && jetify",
    "start": "react-native start --reset-cache",
    "build:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build:android": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "build:android-apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "test": "jest --watch",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "update-mobile-app-version": "node ./update-mobile-app-version.js"
  },
  "dependencies": {
    "@react-native-community/checkbox": "^0.5.8",
    "@react-native-community/datetimepicker": "^6.1.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-community/push-notification-ios": "^1.10.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@sentry/react-native": "^5.1.1",
    "@shopify/flash-list": "^1.4.3",
    "base-64": "^1.0.0",
    "dayjs": "^1.11.2",
    "jetifier": "^2.0.0",
    "nativewind": "^2.0.11",
    "prop-types": "^15.7.2",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-awesome-slider": "1",
    "react-native-bootsplash": "^4.5.3",
    "react-native-calendars": "^1.1284.0",
    "react-native-camera": "^3.30.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^8.4.8",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-in-app-review": "^4.2.1",
    "react-native-localize": "^2.2.4",
    "react-native-mmkv": "^2.6.3",
    "react-native-permissions": "^3.8.4",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.27.0",
    "react-native-speedometer-chart": "^0.7.5",
    "react-native-svg": "^11.0.1",
    "react-native-swipe-list-view": "^3.1.3",
    "react-native-swiper": "^1.6.0-nightly.5",
    "react-native-tab-view": "^3.5.2",
    "react-native-webview": "^13.8.6",
    "react-native-youtube-iframe": "^2.3.0",
    "recoil": "^0.7.3-alpha.2",
    "styled-components": "^5.0.0",
    "urijs": "^1.19.11",
    "uuid": "^3.4.0",
    "validator": "^12.2.0"
