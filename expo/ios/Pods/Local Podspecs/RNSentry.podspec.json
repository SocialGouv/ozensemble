{
  "name": "RNSentry",
  "version": "5.22.3",
  "license": "MIT",
  "summary": "Official Sentry SDK for react-native",
  "authors": "Sentry",
  "homepage": "https://github.com/getsentry/sentry-react-native",
  "source": {
    "git": "https://github.com/getsentry/sentry-react-native.git",
    "tag": "5.22.3"
  },
  "platforms": {
    "ios": "11.0",
    "osx": "10.13",
    "tvos": "11.0",
    "visionos": "1.0"
  },
  "preserve_paths": "*.js",
  "dependencies": {
    "React-Core": [

    ],
    "Sentry/HybridSDK": [
      "8.26.0"
    ],
    "React-Codegen": [

    ],
    "RCT-Folly": [

    ],
    "RCTRequired": [

    ],
    "RCTTypeSafety": [

    ],
    "ReactCommon/turbomodule/core": [

    ],
    "React-hermes": [

    ],
    "hermes-engine": [

    ]
  },
  "source_files": "ios/**/*.{h,m,mm}",
  "public_header_files": "ios/RNSentry.h",
  "compiler_flags": "$(inherited) -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -Wno-comma -Wno-shorten-64-to-32 -DRCT_NEW_ARCH_ENABLED -DSENTRY_PROFILING_SUPPORTED=1",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17"
  }
}
