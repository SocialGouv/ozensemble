{
  "name": "oz-ensemble",
  "version": "1.29.0",
  "scripts": {
    "get-ip": "./get-ip.sh && echo \"this script updates the API IP to your own IP, because that's what Android Emulator needs to work. It's based on your wifi IP, if you need ethernet it won't work (needs en1 instead of en0)\"",
    "start": "expo start --dev-client",
    "prebuild": "/bin/sh -c 'export $(grep -v ^'#' .env.local | xargs) && yarn eas-build-pre-install && npx expo prebuild --clean && rm -f ./google-services.json' && rm -f ./ios/.xcode.env.local",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "lint-fix": "expo lint --fix",
    "eas-build-pre-install": "echo \"This command is also run before `eas build` locally or in the cloud'\" && echo \"This command is also run before `eas build` locally or in the cloud'\" && node generate-google-service-json.js",
    "eas-build-post-install": "rm -f ./ios/.xcode.env.local",
    "load-env": "/bin/sh -c 'export $(grep -v ^'#' .env.production | xargs)'",
    "build-eas:ios": "eas build -p ios",
    "build-eas-then-submit:ios": "eas build -p ios && eas submit -p ios --latest",
    "build-eas:android-aab": "eas build -p android",
    "build-local:ios-ipa": "/bin/sh -c 'export $(grep -v ^'#' .env.production | xargs) && eas build -p ios --local'",
    "build-local-then-submit:ios-ipa": "/bin/sh -c 'export $(grep -v ^'#' .env.production | xargs) && eas build -p ios --local' && eas submit -p ios",
    "build-local:ios-ipa:debug": "echo \"This script retains XCode logs for debugging build failures. Look for a message like 'Skipping cleanup, {path/to/logs} won't be removed.'\" && /bin/sh -c 'export $(grep -v ^'#' .env.production | xargs) && EAS_LOCAL_BUILD_SKIP_CLEANUP=1 eas build -p ios --local'",
    "build-local:android-aab": "/bin/sh -c 'export $(grep -v ^'#' .env.production | xargs) && eas build -p android --local'",
    "expo-config": "npx expo config",
    "update-mobile-app-version": "node ./update-mobile-app-version.js"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "6.6.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.3.2",
    "@sentry/react-native": "~6.3.0",
    "@shopify/flash-list": "1.7.3",
    "base-64": "^1.0.0",
    "dayjs": "^1.11.11",
    "expo": "^52.0.39",
    "expo-application": "~6.0.2",
    "expo-build-properties": "~0.13.2",
    "expo-checkbox": "~4.0.1",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.14",
    "expo-dev-menu": "6.0.21",
    "expo-document-picker": "~13.0.3",
    "expo-file-system": "~18.0.11",
    "expo-image": "~2.0.6",
    "expo-linking": "~7.0.5",
    "expo-notifications": "~0.29.14",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^2.0.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-awesome-slider": "^2.5.3",
    "react-native-device-info": "^11.1.0",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-in-app-review": "^4.3.3",
    "react-native-localize": "^3.2.0",
    "react-native-mmkv": "3.0.2",
    "react-native-pager-view": "^6.4.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-restart": "0.0.27",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-share": "^12.0.9",
    "react-native-svg": "15.8.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-webview": "13.12.5",
    "react-native-youtube-iframe": "^2.3.0",
    "recoil": "^0.7.7",
    "styled-components": "^5.3.11",
    "urijs": "^1.19.11",
    "uuid": "^10.0.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/base-64": "^1",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/react": "~18.3.12",
    "@types/react-native-get-random-values": "^1",
    "@types/semver": "^7",
    "@types/urijs": "^1",
    "@types/uuid": "^10",
    "@types/validator": "^13",
    "babel-preset-expo": "~12.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "knip": "^5.26.0",
    "semver": "^7.6.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.3.3"
  },
  "private": true
}
